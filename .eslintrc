    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["primer-react"],
  "extends": ["standard", "standard-react","plugin:primer-react/recommended"],
  "rules": {
    "jsx-quotes": 0
  },
  "env": {
    "jest": true,
    "browser": true
  }
}